<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net481;net6.0-windows;net8.0-windows</TargetFrameworks>
		<OutputType>Library</OutputType>
		<UseWindowsForms>true</UseWindowsForms>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>
	<PropertyGroup>
		<AssemblyTitle>ZedGraph</AssemblyTitle>
		<Description>ZedGraph Library</Description>
		<Company>John Champion, et al.</Company>
		<Product>ZedGraph Library</Product>
		<Copyright>Copyright Â© 2003-2007 John Champion</Copyright>
		<AssemblyVersion>5.2.0</AssemblyVersion>
		<InformationalVersion>5.2.0</InformationalVersion>
		<SignAssembly>True</SignAssembly>
		<AssemblyOriginatorKeyFile>..\zedgraphkey.snk</AssemblyOriginatorKeyFile>
	</PropertyGroup>

	<ItemGroup>
		<Compile Update="ZedGraphLocale.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>ZedGraphLocale.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
